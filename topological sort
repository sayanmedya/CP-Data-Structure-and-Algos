#include<iostream>
#include<vector>
#include<list>
using namespace std;
// added graph
int arr[5][5]={{0,10,0,5,0},{0,0,1,2,0},{0,0,0,0,4},{0,3,9,0,2},{7,0,6,0,0}};
vector<int> q;
list<int> r;
int time=0;
void dfs(int i,int stime[],int ftime[],int parent[],int color[]);
int main()
{
    int v;
    cout<<"enter number of vertices\n";
    cin>>v;
    int stime[v],ftime[v],parent[v],color[v];
    int i,j;
    for(i=0;i<v;i++)
    {
        color[i]=0;
        parent[i]=-1;
    }
    for(i=0;i<v;i++)
    {
        if(color[i]==0)
        {
                 dfs(i,stime,ftime,parent,color);
        }

    }

    list<int>::iterator it;
    for(it=r.begin();it!=r.end();it++)
    {
        cout<<*it<<endl;
    }
}
void dfs(int i,int stime[],int ftime[],int parent[],int color[])
{
    time++;
    color[i]=1;
    stime[i]=time;
    int j;
    for(j=0;j<5;j++)
    {
        if(arr[i][j]!=0 && color[j]==0)
        {
            dfs(j,stime,ftime,parent,color);
        }
    }
     time++;
    ftime[i]=time;
    color[i]=2;
    r.push_front(i);
}
